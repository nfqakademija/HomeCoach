<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8" />
    <title>{% block title %}{{ "NFQ #5" }}{% endblock %}</title>
    <meta name="viewport" content="width=device-width,initial-scale=1.0">
    <link rel="icon" type="image/x-icon" href="{{ asset('images/favicon.ico')}}">
    {% block stylesheets %}
        <link rel="stylesheet" href="{{ asset('assets/bootstrap.min.css') }}"/>
        <link rel="stylesheet" href="{{ asset('css/headerStyle.css') }}"/>
        <link rel="stylesheet" href="{{ asset('css/bodyStyle.css') }}"/>
        <link rel="stylesheet" href="{{ asset('css/regimeInputFormStyle.css') }}"/>
        <link rel="stylesheet" href="{{ asset('fonts/glyphicons-haflings-regular.ttf') }}">
        <link rel="stylesheet" href="{{ asset('fonts/Oswald-Light.ttf') }}">
        <link rel="stylesheet" href="{{ asset('css/regimeStyle.css') }}">
        <link rel="stylesheet" href="{{ asset('css/commentsStyle.css') }}">
        <link rel="stylesheet" href="{{ asset('css/regimeShowStyle.css') }}">
    {% endblock %}
    {% block canonical_meta %}{% endblock %}
</head>
{% block body %}
    <div id="fb-root"></div>
    <script>(function(d, s, id) {
            var js, fjs = d.getElementsByTagName(s)[0];
            if (d.getElementById(id)) return;
            js = d.createElement(s); js.id = id;
            js.src = "//connect.facebook.net/lt_LT/sdk.js#xfbml=1&version=v2.6&appId=1672063339704987";
            fjs.parentNode.insertBefore(js, fjs);
        }(document, 'script', 'facebook-jssdk'));</script>

    <div class="container">
        {% block container %}
        {% block header %}
            {% include "::inc/header.html.twig" %}
        {% endblock %}
        <div class="container-fluid text-center">
            <div class="row content">
                {% block sideBarLeft %}
                    {% include "::inc/left_column.html.twig" %}
                {% endblock %}
                <div class="col-sm-7 text-left">
                    {% block content %}
                    {% endblock %}
                </div>

                {% block sideBarRight %}
                    {% include "::inc/right_column.html.twig" %}
                {% endblock %}

                {% endblock %}
            </div>
        </div>
    </div>
    {% block footer %}
        {% include "::inc/footer.html.twig" %}
    {% endblock %}
{% block javascripts %}
    <script src="{{ asset('assets/angular.min.js') }}"></script>
    <script src="{{ asset('assets/jquery.min.js') }}"></script>
    <script src="{{ asset('assets/bootstrap.min.js') }}"></script>
    <script src="{{ asset('js/regimeModule.js') }}"></script>
    <script src="{{ asset('js/commentLengthControl.js') }}"></script>
    <script src="{{ asset('js/ratings.js') }}"></script>
{% endblock %}
{% endblock %}
</html>